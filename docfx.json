{
    "metadata": [
      {
        "src": [
          {
            "files": [
              "src/**.csproj"
            ]
          }
        ],
        "dest": "api",
        "disableGitFeatures": false,
        "disableDefaultFilter": false
      }
    ],
    "build": {
      "content": [
        {
          "files": [
            "articles/**.md",
            "articles/**/toc.yml",
            "toc.yml",
            "*.md"
          ]
        },
        {
          "files": [
            "articles/tutorials/**.md",
            "articles/tutorials/**/toc.yml",
            "toc.yml",
            "*.md"
          ]
        },
        {
          "files": [
            "articles/reference/**.md",
            "articles/reference/**/toc.yml",
            "toc.yml",
            "*.md"
          ]
        }        
      ],
      "resource": [
        {
          "files": [
            "images/**",
            "articles/images/**",
            "articles/tutorials/images/**",
            "articles/templates/images/**"
          ]
        }
      ],
      "overwrite": [
        {
          "files": [
            "apidoc/**.md"
          ],
          "exclude": [
            "obj/**",
            "_site/**",
            "README.md"
          ]
        }
      ],
      "dest": "_site",
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": [
        "default"
      ],
      "postProcessors": ["ExtractSearchIndex"],
      "globalMetadata":{
        "_appTitle": "Gaia Platform Documentation",
        "_appLogoPath": "images/gaia-logo.png",
        "_appFaviconPath": "images/gaiafav.ico",
        "_enableSearch": "false",
        "_disableContribution": "true",
        "_appFooter": "Copyright Â© 2021 GAIA Platform. All rights reserved. Declarative C++ is a trademark of GAIA Platform.<br>Generated by <strong>DocFX</strong></span>"
      },
      "markdownEngineName": "markdig",
      "noLangKeyword": false,
      "keepFileLink": false,
      "cleanupCacheHistory": false,
      "disableGitFeatures": false
    }
  }